{
"manifest_version": 2,
    "name": "Turner Translator",
    "description": "Does some page modifications based on certain text and persona",
    "version": "1.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "TurnerTranslator.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "icons" : {
        "16" : "justice.png",
        "48" : "justice.png",
        "128" : "justice.png"
    }
}